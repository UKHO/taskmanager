                    <td>
                        <div class="status inProgress">
                            <span class="fa fa-edit"> </span>
                            <span>In Progress</span>
                        </div>
                    </td>
                    <td>stoodleym</td>
                    <td />
                    <td>12/01/2020</td>
                    <td>
                        <div class="row align-items-center">
                            <div class="col-2 offset-10">

                                <div class="checkbox-wrapper">
                                    <input type="checkbox" id="checkbox2">
