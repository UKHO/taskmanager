                        </div>
                    </td>
                    <td>stoodleym</td>
                    <td />
                    <td>12/01/2020</td>
                    <td>
                        @(await Html.RenderComponentAsync<WorkflowActions>(RenderMode.ServerPrerendered, new { WorkflowStep = "CommitToPrint", Rework = false }))
                    </td>
                </tr>

                @*--- START WORKFLOW TABLE: CIS ---*@
                <tr class="shaded">
                    <td><h4>CIS</h4></td>
                    <td>
                        <div class="status inProgress">
                            <span class="fa fa-edit"> </span>
