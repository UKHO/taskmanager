                            <span class="fa fa-edit"> </span>
                            <span>In Progress</span>
                        </div>
                    </td>
                    <td>stoodleym</td>
                    <td />
                    <td>12/01/2020</td>
                    <td>
                        @(await Html.RenderComponentAsync<WorkflowActions>(RenderMode.ServerPrerendered, new { WorkflowStep = "V2Rework", Rework = false }))
